{
  "version": "0.0.1",
  "license": "MIT",
  "name": "bertie-blackman",
  "description": "My portfolio site",
  "scripts": {
    "clean": "gatsby clean",
    "build": "export $(egrep -v '^#' .env | xargs) && gatsby build",
    "build-ci": "gatsby build",
    "ci": "yarn lint && yarn prettier:check && yarn test && yarn build-ci",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "prettier:check": "prettier --check \"**/*.*\"",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test:ci": "jest --coverage",
    "tsc:repo": "tsc -p tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "NODE_ENV=production build-storybook"
  },
  "dependencies": {
    "@storybook/react": "^5.3.19",
    "gatsby": "^2.23.18",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@testing-library/react": "^10.4.3",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.3",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "classnames": "^2.2.6",
    "cypress": "^4.9.0",
    "date-fns": "^2.14.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.3.8",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-netlify": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-postcss": "^2.3.8",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-typescript": "^2.4.12",
    "gatsby-source-contentful": "^2.3.20",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.2.11",
    "netlify-plugin-cypress": "^1.3.11",
    "prettier": "2.0.5",
    "tailwindcss": "^1.4.6",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
